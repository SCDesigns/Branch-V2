<%= form_for branch do |f| %>
  <%= f.label :category %>
  <%= f.collection_select :category, Category.all, :id, :name %>
  <%= f.label :city %>
  <%= f.collection_select :city, City.all, :id, :name %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :organization %>
  <%= f.text_field :organization %>
  <%= f.label :date %>
  <%= f.text_field :date %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :info %>
  <%= f.text_field :info %>
  <%= f.submit 'Submit Branch' %>
<% end %>
